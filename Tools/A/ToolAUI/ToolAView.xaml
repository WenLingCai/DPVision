<UserControl x:Class="ToolAUI.ToolAView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ToolAUI"
             Height="150" Width="300">
    <StackPanel>
        <TextBox Text="{Binding Parameters.Input, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Width="200" Margin="5"/>
        <Button Content="执行" Command="{Binding ExecuteCommand}" Width="100" Margin="5"/>
        <TextBlock Text="{Binding Parameters.Output}" Margin="5"/>
    </StackPanel>
</UserControl>